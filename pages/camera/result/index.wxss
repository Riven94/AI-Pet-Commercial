/* pages/camera/result/index.wxss */
@import "../../../template/animalcard/animalcard.wxss";
@import "../../../template/ownercard/ownercard.wxss";
page{
  background-color: rgb(243, 237, 237);
}
.container{
    height: 100%;
    width: 100%;
    display: flex;
    flex-direction: column;
}
.topone{
  width: 100%;
  display: flex;
  background-color: rgb(245, 191, 122);
  flex-direction: column;
  align-items: center;
}
.tab {
  float: left;
  text-align: center;
  padding: 15rpx 0;
  display: block;
  border:3px solid white;
  border-radius: 400rpx;
  background: white;
  height: 30rpx;
  margin: 20rpx 40rpx;
}

.topTabSwiper {
  border-top: 1px solid #ccc;
  border-bottom: 1px solid #ccc;
  zoom: 1;
}

.topTabSwiper:after {
  content: "";
  clear: both;
  display: block;
}

.tabBorer {
  border-bottom: 2px solid rgb(243, 237, 237);
  color: orange;
}

.swiper {
  width: 100%;
}

.swiper_con {
  text-align: center;
  width: 100%;
  height: 100%;
  padding: 80rpx 0;
}
