page{
  background-color: #F3BE67;
}
.container{
  width: 100%;
  height: 100%;
}
.icon{
  display: flex;
  align-items: center;
}
.iconImg{
  width: 100rpx;
  height: 100rpx;
}
.top{
  height: 1150rpx;
  margin: 0 auto;
  display: flex;
  flex-direction: column;
  border-radius: 0 0 50rpx 50rpx;
  background-color: white;
}
.text{
  font-size: 33rpx;
}
.desc{
  color: gray;
  display: flex;
  align-items: center;
}
.wrap{
  height: 200rpx;
  display: flex;
  width: 95%;
  justify-content: space-between;
  margin: 50rpx auto;
}
.bottom{
  height: 320rpx;
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.join{
  color: #F3BE67;
  font-size: 35rpx;
  background-color: white;
  padding: 5rpx 15rpx;
  border-radius: 30rpx;
}
.main{
  position: relative;

}
.main-icon{
  width: 500rpx;
  height: 500rpx;
}
.texts{
  font-size: 35rpx;
  position: absolute;
  top: 240rpx;
  right: 30rpx;
  font-family:'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
  font-weight: 700;
}
.main-text{
  margin-top: 8rpx;
}