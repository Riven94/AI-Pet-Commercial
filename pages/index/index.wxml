<!--pages/my/index.wxml-->
<import src="../../template/publishitem/publishitem.wxml"></import>
<view class="container">
  <view class="top">
  <view class="message">
  <text class="newmessage">最新消息</text>
  <view class="more" bindtap="bindViewTap">
  <view class="triangle"></view>
  <text class="moremessage">更多消息</text>
  </view>
  </view>
  <view class="search">
  <image class="searchicon" src="../../icons/search.png"></image>
  </view>
  </view>
  <view bindtap="toMore">
    <template wx:for="{{myPublish}}" is="publishitem" data="{{item}}"></template>
  </view>
</view>
